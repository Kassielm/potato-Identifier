# Configuração do Sistema Potato Identifier

# === CONFIGURAÇÕES DA PLACA TORADEX ===
TORIZON_IP=192.168.100.174
HOST_IP=192.168.100.146
TORIZON_ARCH=aarch64
DOCKER_REGISTRY=registry-1.docker.io
DOCKER_LOGIN=kassiell
APP_ROOT=/home/torizon/app

# === CONFIGURAÇÕES DO PLC ===
PLC_IP=192.168.2.201
PLC_RACK=0
PLC_SLOT=1
PLC_DB_NUMBER=1
PLC_DB_OFFSET=0

# === CONFIGURAÇÕES DA CÂMERA ===
CAMERA_WIDTH=640
CAMERA_HEIGHT=480
CAMERA_PIXEL_FORMAT=RGB8

# === CONFIGURAÇÕES DO MODELO ===
MODEL_EDGETPU=data/models/best_float32_edgetpu.tflite
MODEL_FALLBACK=data/models/best_float32.tflite
LABELS_FILE=data/models/labels.txt
CONFIDENCE_THRESHOLD=0.5

# === CONFIGURAÇÕES DE PERFORMANCE ===
INFERENCE_TIMEOUT_MS=5000
FRAME_PROCESS_INTERVAL_MS=10
ERROR_RETRY_INTERVAL_MS=100
PLC_RETRY_INTERVAL_S=5

# === CONFIGURAÇÕES DE LOGGING ===
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(levelname)s - %(message)s

# === CONFIGURAÇÕES DE DESENVOLVIMENTO ===
DEV_USE_VIRTUAL_DISPLAY=true
DEV_DISPLAY_PORT=:99
DEV_VIRTUAL_RESOLUTION=1024x768x24
